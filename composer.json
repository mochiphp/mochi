{
    "name": "mochiphp/mochi",
    "description": "A MVC framework for slimphp",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "framework",
        "mvc",
        "rapid-development",
        "conventions over configuration",
        "dry",
        "psr-7",
        "slim"
    ],
    "autoload": {
        "psr-4": {
            "Mochi\\": "Mochi/"
        }
    },
    "authors": [
        {
            "name": "Scallyt"
        }
    ],
    "require": {
        "php": "^8",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.0",
        "slim/slim": "4.*",
        "selective/basepath": "^2.2",
        "monolog/monolog": "^3.7",
        "scorninpc/slim-smarty-view": "2.*",
        "odan/session": "6.1.0",
        "symfony/console": "^7.1",
        "illuminate/database": "^11.19",
        "zeuxisoo/slim-whoops": "^0.7.3",
        "robmorgan/phinx": "^0.16.2"
    },
    "bin": "/Mochi/mocho",
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    }
}
